<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E2DFD2"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/toolbar"
        android:background="@color/toolbar"
        app:logo="@drawable/outline_table_bar_24"
        app:title="Dineout"
        app:titleMarginStart="30dp"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/Theme.AppCompat.Light"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="vertical"
        android:layout_below="@id/toolbar">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:id="@+id/s1"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white"
                >

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_account_circle_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Account"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="Security notification , change Account"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s2"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_notifications_none_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Notification"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="Message , group mesaages , calls"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s3"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_favorite_border_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Favorites"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="Added Restaurants , saved"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s4"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_language_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="App Language"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="English(devices' language)"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s5"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_privacy_tip_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Privacy"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="Password , security , Account privacy"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s6"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_device_unknown_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Device Permissions"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="camera , text , location , Notification"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s7"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                android:layout_marginTop="5dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="22dp"
                    android:elevation="3dp"
                    android:src="@drawable/baseline_history_24" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Booking Hestory"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text="Booked Restaurants , Historey"
                    android:textColor="#3e4346"
                    android:textSize="12dp"
                    tools:ignore="TextSizeCheck" />

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/s8"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardElevation="5dp"
                app:cardCornerRadius="5dp"
                android:layout_marginTop="5dp"
                app:cardBackgroundColor="@color/white">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/baseline_call_end_24"
                    android:elevation="3dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="20dp"
                    android:text="Contect us"
                    android:textSize="18dp"
                    android:textColor="#3e4346"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="50dp"
                    android:text=" Phone , Email , text"
                    android:textSize="12dp"
                    android:textColor="#3e4346"/>

            </androidx.cardview.widget.CardView>
<!--            <androidx.cardview.widget.CardView-->
<!--                android:id="@+id/s9"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="40dp"-->
<!--                app:cardElevation="5dp"-->
<!--                android:layout_marginTop="5dp"-->
<!--                app:cardCornerRadius="5dp"-->
<!--                app:cardBackgroundColor="@color/white">-->


<!--                <TextView-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="30dp"-->
<!--                    android:layout_marginLeft="15dp"-->
<!--                    android:layout_marginTop="10dp"-->
<!--                    android:text="Help?"-->
<!--                    android:textSize="18dp"-->
<!--                    android:textColor="#0000FF"/>-->

<!--            </androidx.cardview.widget.CardView>-->


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="20dp"
                    android:textColor="@color/black"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:text="Help?"/>

                <androidx.cardview.widget.CardView
                    android:layout_width="330dp"
                    android:layout_height="40dp"
                    android:id="@+id/s9"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    app:cardBackgroundColor="@color/white"
                    android:layout_marginBottom="15dp"
                    app:cardElevation="20dp"
                    app:cardCornerRadius="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="How we can help?"
                        android:textSize="15dp"
                        android:paddingTop="10dp"
                        android:paddingLeft="7dp"
                        android:textColor="@color/black"/>
                </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/s10"
                android:layout_width="330dp"
                android:layout_height="40dp"
                app:cardElevation="5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="20dp"
                app:cardCornerRadius="5dp"
                app:cardBackgroundColor="@color/white">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:text="Log out "
                    android:textSize="18dp"
                    android:textColor="#ff0000"/>

            </androidx.cardview.widget.CardView>






        </LinearLayout>

    </ScrollView>

</RelativeLayout>